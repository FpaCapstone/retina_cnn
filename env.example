# Backend API Configuration
# Copy this file to .env and update with your values

# Development (local backend)
# EXPO_PUBLIC_API_BASE_URL=http://localhost:3000

# Production (Render deployment)
# Your Render backend URL
# EXPO_PUBLIC_API_BASE_URL=https://retina-cnn.onrender.com

# RETFound API Configuration (optional)
# Development (local)
EXPO_PUBLIC_RETFOUND_API_URL=http://localhost:5000

# Production (update when deployed)
# EXPO_PUBLIC_RETFOUND_API_URL=https://your-retfound-api.com

# Optional: Enable/disable RETFound preference
# EXPO_PUBLIC_PREFER_RETFOUND=true
# EXPO_PUBLIC_FALLBACK_TO_CNN=true
